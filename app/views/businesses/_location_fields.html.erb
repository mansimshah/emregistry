<div class="nested-fields">
  <div class="field">
    <%= f.label :location %><br/>
    <%= f.text_field :location %>
  </div><br/>
  <div class="field">
    <%= f.label :type_of_services %><br/>
    <%= f.text_field :type_of_services %>
  </div><br/>
  <div class="field">
    <%= f.label :description %><br/>
    <%= f.text_area :description %>
  </div><br/>
  <div class="field">
    <%= f.label :cost %><br/>
    <%= f.number_field :cost %>
  </div><br/>
  <div class="field">
    <%= f.label :is_active %><br/>
    <%= f.check_box :is_active %>
  </div><br/>
  <%= link_to_remove_association "Remove Location", f %>
</div>